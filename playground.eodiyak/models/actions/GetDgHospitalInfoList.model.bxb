action (GetDgHospitalInfoList){
  description (진료 과목에 해당하는 병원 리스트를 가져온다.)
  type(Search)
  collect{
    input (nearHospitalList) {
      type (NearHospitalList)
      min (Required) max (Many)
      default-init{
        intent{
          goal: GetHospitalList
        }
      }
    }
    input(dgidIdName){
      type(DgName)
      min (Required) max (One)
    }
  }
  output (HospitalInfo)
}